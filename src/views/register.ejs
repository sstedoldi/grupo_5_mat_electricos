<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include("partials/head") %>
  </head>
  <!-- <div class="top-header-top">Materiales eléctricos</div> -->
  <body>
    <!-- HEADER -->
    <header class="main-header"><%- include("partials/header") %></header>
    <!-- &&&& -->
    <!-- MAIN -->
    <main class="main-register">
      <form action="/users/" method="POST" enctype="multipart/form-data">
      <div class="form-register">
        <h3>Registrate</h3>
          <div  class="controls"><label for="name">Nombre</label>
          <input type="text" name="name" id="name">
          <br></div>

          <div  class="controls"><label for="lastname">Apellido</label>
          <input type="text" name="lastname" id="lastname">
          <br></div>

          <div  class="controls"><label for="years">Edad</label>
          <input type="text" name="years" id="years">
          <br></div>


          <div  class="controls"><label for="email">Email</label>
          <input type="email" name="email" id="email">
          <br></div>

          <div  class="controls"><label for="password">Contraseña</label>
          <input type="password" name="password" id="password">
          <br></div>

          <div class="formularios-create">
            <label for="productImage" class="form-label-create">Imagen de Perfil:</label>
            <input type="file" id="usersImage" name="usersImage">
                
          </div>
            <br />
            <p><a href="#">Estoy de acuerdo con Terminos y Condiciones</a></p>
            <br />
            <input type="submit" value="Registrar" />
        
            <br />
            <p><a href="/users/login">Ya tenes cuenta?</a></p>
          </div>
     </form>
    </main>
    <!-- &&&& -->
    <!-- FOOTER -->
    <footer class="main-footer"><%- include("partials/footer") %></footer>
    <!-- &&&& -->
  </body>
</html>
